<!doctype html>
<html>
    <head>
        <meta http-equiv="content-type" content="text/html; charset=utf-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0" />

        <title><%= locals.title %></title>
        <meta name="description" content="<%= locals.description %>" />

        <meta name="robots" content="index, follow" />
        <meta name="google-adsense-account" content="ca-pub-7324337655317650">
        
        <meta property="og:title" content="<%= locals.title %>" />
        <meta property="og:image" content="<%= locals.imageUrl %>" />
        <meta property="og:description" content="<%= locals.description %>" />

        <link rel="canonical" href="<%= locals.url %>" />

        <link rel="shortcut icon" href="/favicon.ico" type="image/x-icon" />
        <link
            href="https://cdn.jsdelivr.net/npm/bootstrap@latest/dist/css/bootstrap.min.css"
            rel="stylesheet"
            title="bootstrap"
            type="text/css"
        />
        <link
            rel="stylesheet"
            href="/CSS/style.css"
            type="text/css"
            media="all"
        />
        <link
            rel="stylesheet"
            href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.5.0/css/all.min.css"
            title=""
            type=""
        />
    </head>

    <body class="" data-bs-theme="dark">
        <%- include('../Partials/_header.ejs')%>
        <main id="mainContent" class="d-flex flex-column container">
            <%- body %>
        </main>
        <%- include('../Partials/_footer.ejs')%>
    </body>
    <script src="/JS/index.js" type="text/javascript" charset="utf-8"></script>
</html>
